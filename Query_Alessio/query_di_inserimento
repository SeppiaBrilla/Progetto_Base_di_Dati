1) INSERT INTO Videogiochi_e_software.Prodotto (Nome, Anno, Prezzo, Descrizione, Produttore, Sviluppatore, gioco) VALUE (...)
	If gioco
	INSERT INTO Videogiochi_e_software.Gioco (id, Saga) VALUE (gioco, null)

2) INSERT INTO Videogiochi_e_software.Saga (Nome, Descrizione, Stato) VALUE (...)
	UPDATE Videogiochi_e_software.Gioco
	SET Videogiochi_e_software.Gioco.Saga = "nome_nuova_saga" 
	WHERE Videogiochi_e_software.Gioco.id = "id_del_gioco_nella_saga" 

3) INSERT INTO Videogiochi_e_software.Store_PC(Nome, Compagnia) VALUE(...) 
   INSERT INTO Videogiochi_e_software.Console(Nome, Data_Uscita, Supporto, Compagnia, Generazione) VALUE(...)
   
4) INSERT INTO Videogiochi_e_software.Generazione(Numero) VALUE (...)
	INSERT INTO Videogiochi_e_software.Console(Nome, Data_Uscita, Supporto, Compagnia, Generazione) VALUE(...)
	
5) INSERT INTO Videogiochi_e_software.Recensione(Nome, anno, Utente, Voto)(...)

6) UPDATE Videogiochi_e_software.Prodotto
	SET Videogiochi_e_software.Prodotto.[...] = [...]
	WHERE Videogiochi_e_software.Prodotto.Gioco = "id_del_gioco"

7) INSERT INTO Videogiochi_e_software.Categoria(Nome, Derivazione) VALUE (...)
	INSERT INTO Videogiochi_e_software.Tipologia(Nome_Prodotto, Anno, Nome_Categoria) VALUE(...)

8) INSERT INTO Videogiochi_e_software.Utente(Nickname) VALUE(...)

9) SELECT Nome
	FROM Categoria
	WHERE Categoria.Derivazione = "genere_padre"

10) SELECT Compagnia.Nome AS Nome, Compagnia.Fondazione AS Fondazione, Compagnia.Nazionalita AS Nazionalita
	FROM Designer, Progettazione Gioco, Prodotto, Compagnia
	WHERE Designer.Id = Progettazione.Id_Designer AND Progettazione.Id_Gioco AND Gioco.Id = Prodotto.Gioco AND Prodotto.Produttore = Compagnia.Nome AND Prodotto.Sviluppatore = Compagnia.Sviluppatore AND Designer.Id = "id"  
